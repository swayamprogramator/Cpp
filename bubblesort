#include<iostream>
using namespace std;
void bubble(int arr[],int n){
    int i,j,temp;
    for(i=0;i<n-1;i++){
        for(j=0;j<n-i-1;j++){
            if(arr[j]>arr[j+1]){
              int temp=arr[j];
              arr[j]=arr[j+1];
              arr[j+1]=temp;
            }
        }
    }
}
int main(){
    int n;
    printf("Enter no. of element you want:");
    scanf("%d",&n);
    int arr[n];
    printf("Enter Element:\n");
    for (int i=0;i<n;i++){
        cin>>arr[i];
    }
    bubble(arr,n);
   for (int i=0;i<n;i++){
        cout<<arr[i]<<"\t";
    }
    return 0;
}
